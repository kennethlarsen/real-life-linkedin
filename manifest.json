{
"manifest_version": 2,
    "name": "Real Life LinkedIn",
    "description": "Discover the true meaning of what people say on LinkedIn",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://www.linkedin.com/*",
								"*://linkedin.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
